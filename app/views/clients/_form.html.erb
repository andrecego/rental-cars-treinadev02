<%= render  'shared/errors', object: client %>
<%= form_for client do |f| %>
  <%= f.label :name, 'Nome' %>
  <%= f.text_field :name %>
  <%= f.label :document, 'CPF' %>
  <%= f.text_field :document %>
  <%= f.label :email, 'Email' %>
  <%= f.text_field :email %>
  <%= f.submit 'Enviar' %>
<% end %>